{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "containerPort": 80,
            "essential": true,
            "image": "Dockerfile",
            "memory": 512,
            "mountPoints": [
                {
                    "containerPath": "/var/log/nginx",
                    "sourceVolume": "awseb-logs-disco-bot"
                }
            ],
            "name": "disco-bot",
            "portMappings": [
                {
                    "hostPort": 80
                }
            ]
        }
    ]
}